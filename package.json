{
  "name": "underwater-game",
  "version": "2.0.0",
  "description": "Aquatic 3D HTML5 game of survival, fighting underwater creatures in a web browser.",
  "main": "client/index.html",
  "scripts": {
    "test": "npm run eslint && jest --coverage",
