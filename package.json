  "bugs": {
    "url": "https://github.com/ebabel-games/underwater-game/issues"
  },
  "homepage": "https://github.com/ebabel-games/underwater-game#readme",
  "dependencies": {
    "compression": "^1.7.3",
    "ebabel": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.4",
    "eslint": "^5.7.0",
    "jest": "^24.9.0",
